{
    "emptyLines": {},
    "sameLine": {
        "anonFunctionBody": "keep",
        "functionBody": "keep"
    },
    "whitespace": {
        "addLineCommentSpace": true,
        "binopPolicy": "around",
        "compressSuccessiveParenthesis": true
    },
    "wrapping": {
        "opAddSubChain": {
            "defaultWrap": "keep"
        },
        "callParameter": {
            "defaultWrap": "keep",
            "defaultLocation": "afterLast"
        },
        "casePattern": {
            "defaultWrap": "noWrap"
        }
    }
}